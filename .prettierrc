{
    "plugins": ["@prettier/plugin-php", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.blade.php",
            "options": {
                "parser": "html"
            }
        }
    ],
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5"
}
